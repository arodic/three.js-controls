{
  "extends": "mdcs",
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "rules": {
    "no-console": "off",
    "no-console": "off",
    "no-var": "error",
    "semi": "error",
    "strict": "error"
  },
  "env": {
    "browser": true,
    "es6": true
  }
}
